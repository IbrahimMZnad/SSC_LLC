<odoo>
    <data noupdate="1">

        <record id="ir_cron_zkteco_sync" model="ir.cron">
            <field name="name">ZKTeco Cloud Attendance Sync</field>
            <field name="model_id" ref="zkteco_cloud.model_zkteco_attendance_sync"/>
            <field name="state">code</field>
            <field name="code">model.sync_zkteco_attendance()</field>
            <field name="interval_number">5</field>
            <field name="interval_type">minutes</field>
            <field name="max_number_of_calls">-1</field>
            <field name="doall" eval="False"/>
            <field name="active" eval="True"/>
        </record>

    </data>
</odoo>
